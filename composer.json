{
    "name": "niiknow/anx-api-proxy",
    "description": "AppNexus Proxy API",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "darkaonline/l5-swagger": "^8.3",
        "f3ath/appnexus": "^0.4.0",
        "facebook/php-business-sdk": "^13.0",
        "glaivepro/cachepage": "^1.5",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/framework": "^9.2",
        "laravel/tinker": "^2.7",
        "league/csv": "^9.8",
        "league/flysystem-aws-s3-v3": "^3.0.13",
        "maatwebsite/excel": "^3.1",
        "niiknow/laratt": "^2.0.5",
        "rachidlaasri/laravel-installer": "^4.1.0",
        "zircote/swagger-php": "^4.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "filp/whoops": "^2.14",
        "friendsofphp/php-cs-fixer": "^3.8",
        "laravel/sail": "^1.13",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\": "api/",
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "phpcs": "phpcs",
        "phpcs:fix": "phpcbf -w",
        "app:clear": [
            "rm -f dist.tar",
            "rm -f bootstrap/cache/config.php",
            "rm -f storage/framework/sessions/*",
            "php artisan clear-compiled",
            "php artisan cache:clear",
            "php artisan config:clear",
            "php artisan route:clear",
            "php artisan view:clear",
            "composer dump-autoload"
        ],
        "app:package": [
            "composer app:clear",
            "rm -f storage/framework/sessions/*",
            "npm run production",
            "chmod -R 775 bootstrap/",
            "chmod -R 775 storage/",
            "composer install --no-dev --optimize-autoloader",
            "composer archive",
            "mv dist-*.tar dist.tar",
            "tar -rvf dist.tar vendor"
        ],
        "docker:clear": [
            "rm -rf .docker/data/mysql/*",
            "rm -rf .docker/logs/*"
        ],
        "auto-complete:generate": [
            "php artisan ide-helper:meta",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:models --nowrite"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "archive": {
        "name": "dist",
        "exclude": [
            ".docker/",
            "coverage/",
            "dist/",
            "playbook/",
            "public/.tmb/",
            "node_modules/",
            "storage/debugbar/",
            "storage/logs/laravel.log",
            "node_modules/",
            "tests/",
            "vendor/",
            ".DS_Store",
            ".env",
            ".php_cs.cache",
            ".phpunit.result.cache",
            "*.md",
            "*.xml",
            "webqueue.sh"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
